#include<stdio.h>

void main()
{
	int i=1,n,k;
	int Even_sum=0;
	int Odd_sum=0;
	printf("Enter a value:");
	scanf("%d",&n);
	printf("Enter b value:");
	scanf("%d",&k);
	while(i<=n)
	{
		if(i%k==0)
		{
		printf("%d\t",i);
		
		if(i%2==0){
		Even_sum+=i;
 }
		else{
	    Odd_sum+=i;   
	}
}
	    	i++;	
}
	printf("\nEvensum=%d\nOddsum=%d\n",Even_sum,Odd_sum);
	return 0;
}
